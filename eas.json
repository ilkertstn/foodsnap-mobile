{
    "cli": {
        "version": ">= 7.0.0",
        "appVersionSource": "local"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "distribution": "internal",
            "env": {
                "EXPO_PUBLIC_API_BASE": "https://foodsnap-api.vercel.app"
            }
        },
        "production": {
            "distribution": "store",
            "env": {
                "EXPO_PUBLIC_API_BASE": "https://foodsnap-api.vercel.app",
                "EXPO_NO_DOCTOR": "true"
            }
        }
    },
    "submit": {
        "production": {}
    }
}